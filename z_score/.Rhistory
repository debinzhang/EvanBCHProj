library(ggplot2)
library(dplyr)
getZScore <- function(data, feature) {
zscore_data<-data %>% group_by(Dataset) %>% mutate(zscore=({{feature}} - mean({{feature}})) / sd({{feature}}))
#head(zscore_data)
u<-ggplot(data=zscore_data, aes(x=Dataset, y=zscore, fill=Dataset))
u <- u + ylim(-2.5, 2.5)
u + geom_boxplot()
}
